{
    "name": "webinertia/bootstrap",
    "description": "Provides Bootstrap loading, form elements and view helpers for laminas view and form",
    "type": "library",
    "license": "BSD-3",
    "keywords": [
        "laminas",
        "bootstrap",
        "javascript",
        "laminas form",
        "laminas view"
    ],
    "homepage": "https://github.com/webinertia/bootstrap",
    "authors": [
        {
            "name": "Joey Smith",
            "email": "jsmith@webinertia.net",
            "homepage": "https://webinertia.net/"
        }
    ],
    "suggest": {
        "laminas/laminas-component-installer" : "^3.0, required for configuration injection"
    },
    "require": {
        "php": "^8.0 || ^8.1",
        "laminas/laminas-component-installer" : "^3.0",
        "laminas/laminas-mvc": "^3.3.4",
        "laminas/laminas-mvc-i18n": "^1.2.0",
        "laminas/laminas-mvc-form": "^2.0"
    },
    "extra": {
        "laminas": {
            "module": "Bootstrap",
            "config-provider": "Bootstrap\\ConfigProvider"
        }
    },
    "autoload": {
        "psr-4": {
            "Bootstrap\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "laminas/laminas-component-installer": true
        }
    }
}
